	<queryString>
		<![CDATA[SELECT p.m_product_id as p_id, 'Stock real: ' || p.value as etiqueta1, 'Status de compras: ' || p.value as etiqueta2
  FROM m_product p
WHERE p.ad_client_id IN ($P!{USER_CLIENT})
AND p.issold = 'Y'
AND p.isstocked = 'Y'
AND p.ispurchased = 'Y'
AND p.producttype = 'I'
$P!{aux_prod_id}
$P!{aux_formato}
$P!{aux_tipopiedra}
$P!{aux_acabado}
$P!{aux_color}
$P!{aux_pais}
$P!{aux_uso}
$P!{aux_areauso}
$P!{aux_outlet}
;]]>
	</queryString>
